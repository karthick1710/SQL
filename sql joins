SQL> connect system
Enter password:
Connected.
SQL> create table cseitstudent(studentID number primary key,sname varchar(30),department char(5),sem number);

Table created.

SQL> create table placement(PlacementID number primary key,StudentID number,department char(5),company varchar2(30),salary number);

Table created.

SQL>  INSERT INTO cseitstudent (studentID, sname, department, sem) VALUES(101,'reema', 'IT',5);

